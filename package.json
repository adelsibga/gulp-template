{
  "name": "gulp-template",
  "version": "1.0.0",
  "description": "gulp template",
  "scripts": {
    "start": "gulp",
    "prod": "gulp build --prod",
    "lint": "run-s lint:*",
    "lint:stylelint": "stylelint 'src/{scss,sass,less}/**/*.{scss,sass,less}'",
    "lint:eslint": "eslint 'src/js/**/*.js'",
    "lint:editorconfig": "editorconfig-checker",
    "lint:htmlhint": "htmlhint 'build/**/*.html'",
    "fix": "run-s fix:*",
    "fix:stylelint": "stylelint 'src/{scss,sass,less}/**/*.{scss,sass,less}' --fix",
    "fix:eslint": "eslint 'src/js/**/*.js' --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adelsibga/gulp-template.git"
  },
  "author": "Adel Sibgatullin <adelsibga@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adelsibga/gulp-template/issues"
  },
  "homepage": "https://github.com/adelsibga/gulp-template#readme",
  "type": "module",
  "keywords": [
    "gulp",
    "twig",
    "scss",
    "sass",
    "less",
    "linters"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "browser-sync": "^2.29.3",
    "caniuse-lite": "^1.0.30001546",
    "concurrently": "^8.2.1",
    "del": "^6.0.0",
    "editorconfig-checker": "^5.1.1",
    "eslint": "^8.50.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-avif": "^1.1.1",
    "gulp-cssbeautify": "^3.0.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-less": "^5.0.0",
    "gulp-newer": "^1.4.0",
    "gulp-notify": "^4.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-rigger": "^0.5.8",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-strip-css-comments": "^2.0.0",
    "gulp-ttf2woff2": "^4.0.1",
    "gulp-twig": "^1.2.0",
    "gulp-uglify": "^3.0.2",
    "gulp-webp": "^4.0.1",
    "htmlhint": "^1.1.4",
    "notify-send": "^0.1.2",
    "postcss-less": "^6.0.0",
    "postcss-sass": "^0.5.0",
    "postcss-scss": "^4.0.9",
    "sass": "^1.68.0",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
